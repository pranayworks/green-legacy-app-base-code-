{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "react-jsx",
    "lib": ["es2020", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "server"],
  "extends": "expo/tsconfig.base"
}
